{
  "version": 2,
  "builds": [
    {
      "src": "index1/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "style/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "images/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/index1/(.*)",
      "dest": "/index1/$1"
    },
    {
      "src": "/style/(.*)",
      "dest": "/style/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    }
  ]
}
